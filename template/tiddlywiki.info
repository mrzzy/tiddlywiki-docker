{
    "description": "Basic client-server edition",
    "plugins": [
        "tiddlywiki/tiddlyweb",
        "tiddlywiki/filesystem",
        "tiddlywiki/highlight",
        "tiddlywiki/codemirror",
        "tiddlywiki/browser-storage",
        "tiddlywiki/katex",
        "tiddlywiki/markdown",
        "tiddlywiki/mobiledragdrop",

        "tw5-relink/plugins/relink",
        "tw5-relink/plugins/relink-markdown",
        "tw5-relink/plugins/relink-titles",

        "tw5-extendedit/plugin",

        "notebook/plugins/notebook-mobile",

        "tw5-contextplugin/plugins/contextPlugin"
    ],
    "themes": [
        "tiddlywiki/vanilla",
        "tiddlywiki/snowwhite",

        "notebook/themes/notebook"
    ],
    "build": {
        "index": [
            "--rendertiddler",
            "$:/plugins/tiddlywiki/tiddlyweb/save/offline",
            "index.html",
            "text/plain"
        ],
        "static": [
            "--rendertiddler",
            "$:/core/templates/static.template.html",
            "static.html",
            "text/plain",
            "--rendertiddler",
            "$:/core/templates/alltiddlers.template.html",
            "alltiddlers.html",
            "text/plain",
            "--rendertiddlers",
            "[!is[system]]",
            "$:/core/templates/static.tiddler.html",
            "static",
            "text/plain",
            "--rendertiddler",
            "$:/core/templates/static.template.css",
            "static/static.css",
            "text/plain"
        ]
    }
}
